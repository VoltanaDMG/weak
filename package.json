{
  "name": "weak",
  "description": "Make weak references to JavaScript Objects.",
  "author": "Finn Brandes <oss@derikmediagroup.xyz>",
  "contributors": [
    "Anna Henningsen <anna@addaleax.net>",
    "Ben Noordhuis <info@bnoordhuis.nl>",
    "Nathan Rajlich <nathan@tootallnate.net>"
  ],
  "version": "1.0.3",
  "main": "lib/weak.js",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "nyc mocha -gc"
  },
  "binary": {
        "module_name" : "weakref",
        "module_path" : "./lib/binding/napi-v{napi_build_version}",
        "remote-path" : "./{name}/v{version}/{configuration}/",
        "package_name": "{platform}-{arch}-napi-v{napi_build_version}.tar.gz",
        "host"        : "https://localhost/registry/weak/",
        "napi_versions": [1,3]
  },
  "repository": {
    "type": "git",
    "url": "git://gitlab.com/Derik_FB/weak.git"
  },
  "dependencies": {
    "bindings": "^1.3.0",
    "node-addon-api": "^1.1.0",
    "node-pre-gyp": "^0.13.0",
    "setimmediate-napi": "^1.0.3"
  },
  "devDependencies": {
    "mocha": "^5.0.0",
    "nyc": "^11.3.0"
  },
  "keywords": [
    "weak",
    "reference",
    "js",
    "javascript",
    "object",
    "function",
    "callback",
    "napi"
  ],
  "license": "MIT"
}